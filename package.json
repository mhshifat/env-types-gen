{
	"name": "@mhshifat/env-types-gen",
	"version": "1.0.10",
	"description": "A simple script to generate types from a '.env' file in your project.",
	"main": "./build/index.js",
	"files": [
		"build"
	],
	"bin": {
		"env-types-gen": "./build/index.js"
	},
	"scripts": {
		"start": "node ./build/index.js",
		"env-types-gen": "./build/index.js",
		"serve": "ts-node src/index.ts generate",
		"build": "tsc -p .",
		"generate": "env-types-gen generate"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@mhshifat/env-types-gen": "^1.0.10",
		"@types/node": "^14.14.27",
		"nodemon": "^2.0.7",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"chalk": "^4.1.0"
	}
}
